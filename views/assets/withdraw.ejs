<script>
    $('.dropdown').dropdown();
    "<%allCoins = JSON.stringify(allCoins);%> ";
    document.getElementById("topleftLoading").style.display = "block";
    var allCoins = '<%- allCoins %>';
    allCoins = JSON.parse(allCoins);
    var allData = '<%- everything %>';
    allData = JSON.parse(allData);
    let data = allData;
    let coin = '<%-coin%>';
    if (coin !== "none"){
        let html = document.getElementById(coin+"-dropdown-item").innerHTML;
        console.log(html)
        document.querySelectorAll(".selection.dropdown .text")[0].innerHTML = html
    }
    var currentCoin = "none";
    document.querySelector("[name='coin-selector']").addEventListener("change", function(){
        Object.entries(allCoins).forEach(([key, value]) => {
            key = key.toLowerCase();
            if (document.querySelector("[name='coin-selector']").value.toLowerCase().includes(key)){
                currentCoin = key.toUpperCase();
                return;
            }
        })
    })
    var minWithdrawals = {
        ETH: 0.00037,
        LTC: 0.0069,
        BTC: 0.00012,
        XRP: 21,
        DOGE: 10,
        ETC: 0.1
    }
    
    document.getElementById("amount-input").placeholder = "Min withdraw: "+minWithdrawals[coin]+" "+coin

    document.addEventListener("keydown", function(event){
        if (event.key === "Enter" && currentCoin !== "none"){
            window.location.href = "/assets/deposit/"+currentCoin;
        }
    })
    var observer = new IntersectionObserver(function(entries) {
	if(entries[0].isIntersecting === true)
        {document.querySelectorAll(".secondary-header")[0].style = "max-height: 50px; color: #b7b7b7; overflow: hidden;";
    document.getElementById("topleftLoading").classList.remove("loadingGiftopLeftactive")}
    else {
        document.querySelectorAll(".secondary-header")[0].style = "max-height: 50px; color: #b7b7b7; overflow: hidden; background: #1D2946; position: fixed; z-index: 1; top: 0; padding-top: 10px;";
        document.getElementById("topleftLoading").classList.add("loadingGiftopLeftactive")}
    }, { threshold: [0] });

    observer.observe(document.querySelector(".u-logo-image"));
    async function copy(elem) {
        let text = elem.innerText;
        await navigator.clipboard.writeText(text);
        showAlertModal("<i class='fas fa-check'></i> copied!"); 
    }
    function withdraw() {
        var currentcoin = coin.toLowerCase();
        var address = document.getElementById("withdraw-input").value;
        var tag;
        if (currentcoin === "xrp") tag = document.getElementById("xrp-tag").value;
        var amount = Number(document.getElementById("amount-input").value);
        if (address === "") return document.querySelector("[for='withdraw-input']").style.opacity = "1";
        if (document.getElementById("amount-input").value === "") return document.querySelector("[for='amount-input']").style.opacity = "1";
            // do some extra validation
        var maxAmount = Number(document.getElementById("max-withdrawal").innerText);
        if (maxAmount < amount) return showAlertModal("You don't have enough funds for this transaction", {backgroundColor: 'red', waitTime: 2000})


        var xhr = new XMLHttpRequest();
        xhr.open('POST', '/withdraw/' + currentcoin);
        xhr.setRequestHeader('Content-Type', 'application/json');

        xhr.onload = function () {
            console.log(xhr.response);

            if (xhr.response.includes("good")) {
                showAlertModal("Check your email for a verification code! Further instructions are located there.", {waitTime: 3000})
            } else {
                return showAlertModal(xhr.response, {backgroundColor: 'red', waitTime: 2000})
            }
            let inputs = document.querySelectorAll("input")
            for (let i = 0; i<inputs.length; i++){
                inputs[i].value = ""
            }
        };


        xhr.send(JSON.stringify({
            address: address,
            amount: amount,
            tag: tag
        }));
    }
    async function showAlertModal(text, params){
        let modal = document.querySelector(".alert-modal");
        if ((params) && params.backgroundColor) modal.style.backgroundColor = params.backgroundColor;
        var waitTime = 1000;
        if (params && params.waitTime) waitTime = params.waitTime; 
        modal.innerHTML = text;
        modal.style.transform = "translateY(400px)";
        await sleep(waitTime);
        modal.style.transition = "all cubic-bezier(1, 0.03, 1, 1) 1s";
        modal.style.opacity = "0.1";
        modal.style.transform = "translateY(100vh)";
        await sleep(1000)
        var el = modal;
        elClone = el.cloneNode(true);
        elClone.style = "";
        el.parentNode.replaceChild(elClone, el);
    }
    document.addEventListener("keyup", function(event){
        if (event.key === "/"){
            document.querySelectorAll(".search")[1].select()
        }
    })
    document.addEventListener("keydown", function(event){
        if (event.key.includes("Arrow")){
            document.querySelectorAll(".search")[1].select()
        }
    })
    function emptyFunction(){
        return null;
    }
    function goToHomePage(){
        var websites = {
            BTC: "https://bitcoin.org/en/",
            ETH: "https://ethereum.org/",
            LTC: "https://litecoin.com/en/",
            XRP: "https://ripple.com/",
            DOGE: "https://dogecoin.com/",
            ETC: "https://ethereumclassic.org/"
        }
        window.open(websites[coin])
    }
    var withdrawInput = false;
    var amountInput = false;
    document.getElementById("withdraw-input").addEventListener("click", function(){
        withdrawInput = true;
    })
    document.getElementById("amount-input").addEventListener("click", function(){
        amountInput = true;
    })
    document.addEventListener('click', function(event) {
        if (withdrawInput === true){
            var isClickInsideElement = document.getElementById("withdraw-input").contains(event.target);
            if (!isClickInsideElement) {
                if (document.getElementById("withdraw-input").value === ""){
                    document.getElementById("withdraw-input").style.border = "solid 1px red";
                    document.querySelector("[for='withdraw-input']").style.opacity = "100%"
                }else{
                    document.querySelector("[for='withdraw-input']").style.opacity = "0";
                    document.getElementById("withdraw-input").style.border = "solid 1px #24AE8F";
                }
            }
        }
        if (amountInput === true){
            var isClickInsideElement = document.getElementById("amount-input").contains(event.target);
            if (!isClickInsideElement) {
                if (document.getElementById("amount-input").value === ""){
                    document.getElementById("amount-input").style.border = "solid 1px red";
                    document.querySelector("[for='amount-input']").style.opacity = "100%"
                }else{
                    document.querySelector("[for='amount-input']").style.opacity = "0";
                    document.getElementById("amount-input").style.border = "solid 1px #24AE8F";
                }
            }
        }
    });
    document.addEventListener("keydown", function(event){
        if (withdrawInput === true){
            if (document.getElementById("withdraw-input").value === ""){
                document.getElementById("withdraw-input").style.border = "solid 1px red";
                document.querySelector("[for='withdraw-input']").style.opacity = "100%"
            }else{
                document.querySelector("[for='withdraw-input']").style.opacity = "0";
                document.getElementById("withdraw-input").style.border = "solid 1px #24AE8F";
            }
        }
        if (amountInput === true){
            if (document.getElementById("amount-input").value === ""){
                document.getElementById("amount-input").style.border = "solid 1px red";
                document.querySelector("[for='amount-input']").style.opacity = "100%"
            }else{
                document.querySelector("[for='amount-input']").style.opacity = "0";
                document.getElementById("amount-input").style.border = "solid 1px #24AE8F";
            }
        }
    })
    var balances = {
        btc: 0,
        eth: 0,
        ltc: 0,
        xrp: 0
    }
    getMax();
    function getMax(){
        var functions = {
            BTC: getBtcData,
            LTC: getLtcData,
            ETH: getEthData,
            XRP: getXrpData,
            DOGE: getDogeData,
            ETC: getEtcData
        }
        let runFunction = functions[coin];
        runFunction();
    }
    function dataCallback(){
        document.getElementById("topleftLoading").style.display = "none";
        let balance = balances[coin.toLowerCase()];
        console.log(balance);
        document.getElementById("coin-amount").innerText = balance;
        let max = Number(balance)-minWithdrawals[coin];
        if (max < 0) max = 0;
        document.getElementById("max-withdrawal").innerText = max
    }
    async function getBtcData() {
        let results = await fetch("https://api.blockcypher.com/v1/btc/main/addrs/" + data["btc"].address + "/balance");
        results = await results.json();
        if (results.error) balances.btc = 0;
        else balances.btc = results["balance"] * 0.00000001;
        return dataCallback();
    }
    async function getLtcData() {
        let results = await fetch("https://api.blockcypher.com/v1/ltc/main/addrs/" + data["ltc"].address + "/balance");
        results = await results.json();
        if (results.error) balances.ltc = 0;
        else balances.ltc = results["balance"] * 0.00000001;
        return dataCallback();
    }
    async function getEthData() {
        let results = await fetch("/api/v2/explorer/eth/auto/" + data["eth"].address);
        results = await results.json();
        for (let i = 0; i<10; i++){
            if (results.status === "ok") break;
            results = await fetch("/api/v2/explorer/eth/auto/" + data["eth"].address);
            results = await results.json();
        }
        if (results.status === "failed") balances.eth = 0;
        else balances.eth = results["data"]["wei"] * 0.000000000000000001;
        let ethGas = await fetch("/api/v2/explorer/eth/gasPrice");
        ethGas = Number(await ethGas.text());
        ethGas = ethGas*0.000000001;
        let amountOfGas = 23000 * ethGas * 0.000000001;
        minWithdrawals.ETH = amountOfGas;
        return dataCallback();
    }
    async function getXrpData() {
        let results = await fetch("/api/v2/explorer/xrp/address/" + data["xrp"].address);
        results = await results.json();
        if (results.status === "failed") balances.xrp = 0;
        else balances.xrp = Number(results["data"]["xrpBalance"]);
        return dataCallback();
    }
    async function getDogeData() {

    }
    async function getEtcData() {

    }
</script>
