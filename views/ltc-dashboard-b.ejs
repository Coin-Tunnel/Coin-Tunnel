  window.location.href = "/dashboard-b"
  }else{
  alert(xhr.response)
  }
};
//document.getElementById("custId").value = googleUser.getAuthResponse().id_token;
//var wait=setTimeout("document.hidden.submit();",2000);
xhr.send(JSON.stringify({
email: email
}));
  }

